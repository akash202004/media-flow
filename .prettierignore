# Ignore node_modules directory
node_modules/

# Ignore dist or build directories
dist/
build/

# Ignore coverage reports
coverage/

# Ignore logs and temporary files
logs/
temp/
*.log
*.tmp

# Ignore editor-specific files
.vscode/
.idea/

# Ignore configuration files
.env
.env.local
.env.development
.env.test
.env.production

# Ignore git-related files
.git/
.gitignore

# Ignore npm-related files
package-lock.json
yarn.lock

# Ignore other generated or compiled files
*.map
*.min.*
*.bundle.*
*.tgz
